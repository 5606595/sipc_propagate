    <meta charset="UTF-8">
    <title>报名页面</title>
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="yes" name="apple-touch-fullscreen">
    <meta content="telephone=no,email=no" name="format-detection">
    <link rel="stylesheet" href="/css/baoming.css">
</head>

<body>
    <div class="bg">
        <img src="/imgs/bg10.jpeg" alt="">
        <div class="cover">

        </div>
    </div>
    <div id="form">
        <h2>
            学生组织纳新
        </h2>
        <div class="container">
            <div class="name">
                <i class="iconfont icon-yonghu"></i>
                <input type="text" name="name" value="" placeholder="姓名">
            </div>
            <div class="want1">
                <i class="iconfont icon-weibiaoti110"></i>
                <span>
                    第一志愿
                </span>
                <select name="first_all" class="first">
                    <option value="">
                        请选择
                    </option>
                    <option value="1">
                        学生创新实践中心
                    </option>
                    <option value="2">
                        团委
                    </option>
                    <option value="3">
                        勤工助学中心
                    </option>
                    <option value="4">
                        学生会
                    </option>
                    <option value="5">
                        新闻中心
                    </option>
                </select>
                <select name="first_detail" class="second">
                    <option value="">
                        请选择
                    </option>
                </select>
            </div>
            <div class="want2">
                <i class="iconfont icon-weibiaoti110"></i>
                <span>
                    第二志愿
                </span>
                <select name="second_all" class="first">
                    <option value="">
                        请选择
                    </option>
                    <option value="1">
                        学生创新实践中心
                    </option>
                    <option value="2">
                        团委
                    </option>
                    <option value="3">
                        勤工助学中心
                    </option>
                    <option value="4">
                        学生会
                    </option>
                    <option value="5">
                        新闻中心
                    </option>
                </select>
                <select name="second_detail" class="second">
                    <option value="">
                        请选择
                    </option>
                </select>
            </div>
            <div>
                <i class="iconfont icon-banji"></i>
                <input type="text" name="class" placeholder="专业班级">
            </div>
            <div>
                <i class="iconfont icon-phone"></i>
                <input type="text" name="phone" placeholder="联系电话">
            </div>
            <div class="reason">
                <i class="iconfont icon-guanlishenqingxinxi2"></i>
                <textarea name="reason" id="" placeholder="申报组织理由"></textarea>
            </div>
            <button>
                submit!
            </button>
        </div>
    </div>
</body>
<script>
    var organization = {
        1: ['科技协会'],
        2: ['宣传部', '组织部', '青年志愿者协会', '心理协会'],
        3: ['外联部', '活动部', '管理部', '助贷部'],
        4: ['宣传部', '文艺部', '外联部', '学习部', '办公室', '体育部'],
        5: ['新媒外部', '纸媒部', '摄影协会']
    }
    var first1 = document.querySelector('.want1 .first');
    var first2 = document.querySelector('.want2 .first');
    var second1 = document.querySelector(".want1 .second");
    var second2 = document.querySelector(".want2 .second");
    var option1 = "";
    var option2 = "";
    var num1 = 0, num2 = 0;
    first1.addEventListener("change", function(e) {
        num1 = e.target.value;
        if(num1) {
            var arr = organization[num1];
            option1 = "<option value=''>请选择</option>"
            for(var i = 0, j = arr.length; i < j; i++) {
                option1 += "<option value='" + Number(i + 1) + "'>" + arr[i] + "</option>";
            }
        } else {
            option1 = "<option value=''>请选择</option>"
        }
        second1.innerHTML = option1
    })
    first2.addEventListener("change", function(e) {
        num2 = e.target.value;
        if(num2) {
            var arr = organization[num2];
            option2 = "<option value=''>请选择</option>"
            for(var i = 0, j = arr.length; i < j; i++) {
                option2 += "<option value='" + Number(i + 1) + "'>" + arr[i] + "</option>";
            }
        } else {
            option2 = "<option value=''>请选择</option>"
        }
        second2.innerHTML = option2
    })
</script>

</html>